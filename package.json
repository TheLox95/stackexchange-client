{
  "name": "my-app",
  "version": "0.0.0",
  "license": "MIT",
  "main": "main.js",
  "scripts": {
    "ng": "ng",
    "start": "node server.js",
    "dev": "concurrently \"npm run serve\" \"npm run fake-server\" ",
    "serve": "ng serve --open",
    "build": "ng build --prod",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "concurrently --kill-others \"ng e2e\" \"npm run fake-server\" ",
    "fake-server": "json-server --watch fake-server/api.json --port 4585",
    "fake-server:slow": "json-server --watch fake-server/api.json --port 4585 --delay 5000",
    "electron": "tsc main.ts && ng build --prod && electron .",
    "postinstall": "ng build --aot -prod"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^5.2.0",
    "@angular/common": "^5.2.0",
    "@angular/compiler": "^5.2.0",
    "@angular/core": "^5.2.0",
    "@angular/forms": "^5.2.0",
    "@angular/http": "^5.2.0",
    "@angular/platform-browser": "^5.2.0",
    "@angular/platform-browser-dynamic": "^5.2.0",
    "@angular/router": "^5.2.0",
    "core-js": "^2.4.1",
    "ng2-semantic-ui": "^0.9.6",
    "rxjs": "^5.5.6",
    "zone.js": "^0.8.19",
    "express": "~4.15.2",
    "@angular/cli": "1.6.8",
    "@angular/compiler-cli": "^5.2.0"
  },
  "devDependencies": {
    "@angular/language-service": "^5.2.0",
    "@types/jasmine": "~2.8.3",
    "@types/jasminewd2": "~2.0.2",
    "@types/node": "~6.0.60",
    "codelyzer": "^4.0.1",
    "concurrently": "^3.5.1",
    "electron": "^1.8.2",
    "enhanced-resolve": "^3.3.0",
    "jasmine-core": "~2.8.0",
    "jasmine-spec-reporter": "~4.2.1",
    "json-server": "^0.12.1",
    "karma": "~2.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "^1.2.1",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "~5.1.2",
    "ts-node": "~4.1.0",
    "tslint": "~5.9.1",
    "typescript": "~2.5.3"
  },
  "engines": {
    "node": "6.11.0",
    "npm": "3.10.10"
  }
}
